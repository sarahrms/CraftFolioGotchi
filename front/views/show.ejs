<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Registros<h2>
    <table border="1">
        <thead>
            <tr>
                <td>ID</td>
                <td>Email</td>
                <td>Nome</td>
                <td>Senha</td>
                <td>Acao</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.id %></td>
                <td><%= details.email %></td>
                <td><%= details.usuario %></td>
                <td><%= details.senha %></td>
                <td><a href="/edit/<%= details._id %>">Editar</a><a href="/delete/<%= details.id %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
        <button><a href="/">Voltar</a></button>
</body>
</html>